{
    "name": "croc equilibrium",
    "version": "1.4.1",
    "manifest_version": 2,
	"description": "Баланс ваших списаний в реальном времени, следи за блокировкой интернета и списывай время вовремя",
    "icons": {
					"48": "images/red_48.png"
    },
    "browser_action": {
        "default_title": "browser_action: default_title",
        "default_icon": {
            "48": "images/red_48.png"
        },
        "default_popup": "html/main.html"
    },
    "background": {
					"scripts": [ "js/background.js", "js/vendor/jquery-3.2.1.js", "js/vendor/jquery.signalR-2.2.2.js", "js/signalr/hubs.js" ]
  	},
	"web_accessible_resources": [
		"images/red_48.png",
		"images/green_48.png",
		"images/yellow_48.png",
		"images/gray_48.png"
	],
    "homepage_url": "http://croc.ru/"
}